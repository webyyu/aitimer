{"ast":null,"code":"import { ref, computed, reactive } from 'vue';\nimport { FontAwesomeIcon } from '@fortawesome/vue-fontawesome';\nexport default {\n  name: 'RegisterForm',\n  components: {\n    FontAwesomeIcon\n  },\n  emits: ['switch-mode', 'submit'],\n  setup(props, {\n    emit\n  }) {\n    // Ë°®ÂçïÊï∞ÊçÆ\n    const formData = reactive({\n      phoneNumber: '',\n      nickname: '',\n      password: '',\n      confirmPassword: ''\n    });\n\n    // Ë°®ÂçïÁä∂ÊÄÅ\n    const isLoading = ref(false);\n    const showPassword = ref(false);\n    const showConfirmPassword = ref(false);\n    const errors = reactive({\n      phoneNumber: '',\n      nickname: '',\n      password: '',\n      confirmPassword: ''\n    });\n    const message = reactive({\n      text: '',\n      type: ''\n    });\n\n    // È™åËØÅÊâãÊú∫Âè∑\n    const validatePhoneNumber = () => {\n      const phoneRegex = /^1[3-9]\\d{9}$/;\n      if (!formData.phoneNumber) {\n        errors.phoneNumber = 'ËØ∑ËæìÂÖ•ÊâãÊú∫Âè∑';\n      } else if (!phoneRegex.test(formData.phoneNumber)) {\n        errors.phoneNumber = 'ËØ∑ËæìÂÖ•Ê≠£Á°ÆÁöÑ11‰ΩçÊâãÊú∫Âè∑';\n      } else {\n        errors.phoneNumber = '';\n      }\n    };\n\n    // È™åËØÅÊòµÁß∞\n    const validateNickname = () => {\n      if (formData.nickname && formData.nickname.length > 50) {\n        errors.nickname = 'ÊòµÁß∞ÈïøÂ∫¶‰∏çËÉΩË∂ÖËøá50‰∏™Â≠óÁ¨¶';\n      } else {\n        errors.nickname = '';\n      }\n    };\n\n    // È™åËØÅÂØÜÁ†Å\n    const validatePassword = () => {\n      if (!formData.password) {\n        errors.password = 'ËØ∑ËæìÂÖ•ÂØÜÁ†Å';\n      } else if (formData.password.length < 6) {\n        errors.password = 'ÂØÜÁ†ÅÈïøÂ∫¶Ëá≥Â∞ë6‰Ωç';\n      } else {\n        errors.password = '';\n      }\n      // Â¶ÇÊûúÁ°ÆËÆ§ÂØÜÁ†ÅÂ∑≤Â°´ÂÜôÔºåÈáçÊñ∞È™åËØÅÁ°ÆËÆ§ÂØÜÁ†Å\n      if (formData.confirmPassword) {\n        validateConfirmPassword();\n      }\n    };\n\n    // È™åËØÅÁ°ÆËÆ§ÂØÜÁ†Å\n    const validateConfirmPassword = () => {\n      if (!formData.confirmPassword) {\n        errors.confirmPassword = 'ËØ∑Á°ÆËÆ§ÂØÜÁ†Å';\n      } else if (formData.password !== formData.confirmPassword) {\n        errors.confirmPassword = '‰∏§Ê¨°ËæìÂÖ•ÁöÑÂØÜÁ†Å‰∏ç‰∏ÄËá¥';\n      } else {\n        errors.confirmPassword = '';\n      }\n    };\n\n    // Ë°®ÂçïÊòØÂê¶ÊúâÊïà\n    const isFormValid = computed(() => {\n      return formData.phoneNumber && formData.password && formData.confirmPassword && !errors.phoneNumber && !errors.nickname && !errors.password && !errors.confirmPassword;\n    });\n\n    // ÊòæÁ§∫Ê∂àÊÅØ\n    const showMessage = (text, type = 'error') => {\n      console.log(`${type === 'error' ? '‚ùå' : '‚úÖ'} ${text}`);\n      message.text = text;\n      message.type = type;\n      setTimeout(() => {\n        message.text = '';\n        message.type = '';\n      }, 3000);\n    };\n\n    // Ë°®ÂçïÊèê‰∫§\n    const handleSubmit = async () => {\n      console.log('üîÑ Ê≥®ÂÜåË°®ÂçïÊèê‰∫§');\n\n      // È™åËØÅË°®Âçï\n      validatePhoneNumber();\n      validateNickname();\n      validatePassword();\n      validateConfirmPassword();\n      if (!isFormValid.value) {\n        showMessage('ËØ∑Ê≠£Á°ÆÂ°´ÂÜôÊâÄÊúâÂ≠óÊÆµ');\n        return;\n      }\n      isLoading.value = true;\n      try {\n        const submitData = {\n          phoneNumber: formData.phoneNumber,\n          password: formData.password\n        };\n\n        // Â¶ÇÊûúÂ°´ÂÜô‰∫ÜÊòµÁß∞ÔºåÊ∑ªÂä†Âà∞Êèê‰∫§Êï∞ÊçÆ‰∏≠\n        if (formData.nickname.trim()) {\n          submitData.nickname = formData.nickname.trim();\n        }\n        console.log('üì§ Êèê‰∫§Ê≥®ÂÜåÊï∞ÊçÆ:', {\n          ...submitData,\n          password: '***'\n        });\n        await emit('submit', submitData);\n        showMessage('Ê≥®ÂÜåÊàêÂäüÔºÅ', 'success');\n      } catch (error) {\n        console.error('‚ùå Ê≥®ÂÜåÂ§±Ë¥•:', error);\n        const errorMessage = error.response?.data?.message || error.message || 'Ê≥®ÂÜåÂ§±Ë¥•ÔºåËØ∑ÈáçËØï';\n        showMessage(errorMessage);\n      } finally {\n        isLoading.value = false;\n      }\n    };\n    return {\n      formData,\n      errors,\n      message,\n      isLoading,\n      showPassword,\n      showConfirmPassword,\n      isFormValid,\n      validatePhoneNumber,\n      validateNickname,\n      validatePassword,\n      validateConfirmPassword,\n      handleSubmit,\n      showMessage\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","reactive","FontAwesomeIcon","name","components","emits","setup","props","emit","formData","phoneNumber","nickname","password","confirmPassword","isLoading","showPassword","showConfirmPassword","errors","message","text","type","validatePhoneNumber","phoneRegex","test","validateNickname","length","validatePassword","validateConfirmPassword","isFormValid","showMessage","console","log","setTimeout","handleSubmit","value","submitData","trim","error","errorMessage","response","data"],"sources":["C:\\Users\\wuby\\OneDrive\\Desktop\\backend(1) 3\\timer\\src\\components\\login\\RegisterForm.vue"],"sourcesContent":["<template>\n  <form @submit.prevent=\"handleSubmit\" class=\"auth-form\">\n    <!-- ÊâãÊú∫Âè∑ËæìÂÖ• -->\n    <div class=\"form-group\">\n      <label class=\"form-label\">ÊâãÊú∫Âè∑</label>\n      <div class=\"input-wrapper\">\n        <font-awesome-icon icon=\"phone\" class=\"input-icon\" />\n        <input\n          v-model=\"formData.phoneNumber\"\n          type=\"tel\"\n          class=\"form-input\"\n          :class=\"{ 'error': errors.phoneNumber }\"\n          placeholder=\"ËØ∑ËæìÂÖ•ÊâãÊú∫Âè∑\"\n          maxlength=\"11\"\n          required\n          @input=\"validatePhoneNumber\"\n          @blur=\"validatePhoneNumber\"\n        />\n      </div>\n      <div v-if=\"errors.phoneNumber\" class=\"error-message\">\n        {{ errors.phoneNumber }}\n      </div>\n    </div>\n\n    <!-- ÊòµÁß∞ËæìÂÖ• -->\n    <div class=\"form-group\">\n      <label class=\"form-label\">ÊòµÁß∞ <span class=\"optional\">(ÂèØÈÄâ)</span></label>\n      <div class=\"input-wrapper\">\n        <font-awesome-icon icon=\"user\" class=\"input-icon\" />\n        <input\n          v-model=\"formData.nickname\"\n          type=\"text\"\n          class=\"form-input\"\n          :class=\"{ 'error': errors.nickname }\"\n          placeholder=\"ËØ∑ËæìÂÖ•ÊòµÁß∞\"\n          maxlength=\"50\"\n          @blur=\"validateNickname\"\n        />\n      </div>\n      <div v-if=\"errors.nickname\" class=\"error-message\">\n        {{ errors.nickname }}\n      </div>\n    </div>\n\n    <!-- ÂØÜÁ†ÅËæìÂÖ• -->\n    <div class=\"form-group\">\n      <label class=\"form-label\">ÂØÜÁ†Å</label>\n      <div class=\"input-wrapper\">\n        <font-awesome-icon icon=\"lock\" class=\"input-icon\" />\n        <input\n          v-model=\"formData.password\"\n          :type=\"showPassword ? 'text' : 'password'\"\n          class=\"form-input\"\n          :class=\"{ 'error': errors.password }\"\n          placeholder=\"ËØ∑ËÆæÁΩÆÂØÜÁ†ÅÔºàËá≥Â∞ë6‰ΩçÔºâ\"\n          minlength=\"6\"\n          required\n          @input=\"validatePassword\"\n          @blur=\"validatePassword\"\n        />\n        <button\n          type=\"button\"\n          class=\"password-toggle\"\n          @click=\"showPassword = !showPassword\"\n        >\n          <font-awesome-icon :icon=\"showPassword ? 'eye-slash' : 'eye'\" />\n        </button>\n      </div>\n      <div v-if=\"errors.password\" class=\"error-message\">\n        {{ errors.password }}\n      </div>\n    </div>\n\n    <!-- Á°ÆËÆ§ÂØÜÁ†ÅËæìÂÖ• -->\n    <div class=\"form-group\">\n      <label class=\"form-label\">Á°ÆËÆ§ÂØÜÁ†Å</label>\n      <div class=\"input-wrapper\">\n        <font-awesome-icon icon=\"lock\" class=\"input-icon\" />\n        <input\n          v-model=\"formData.confirmPassword\"\n          :type=\"showConfirmPassword ? 'text' : 'password'\"\n          class=\"form-input\"\n          :class=\"{ 'error': errors.confirmPassword }\"\n          placeholder=\"ËØ∑ÂÜçÊ¨°ËæìÂÖ•ÂØÜÁ†Å\"\n          minlength=\"6\"\n          required\n          @blur=\"validateConfirmPassword\"\n        />\n        <button\n          type=\"button\"\n          class=\"password-toggle\"\n          @click=\"showConfirmPassword = !showConfirmPassword\"\n        >\n          <font-awesome-icon :icon=\"showConfirmPassword ? 'eye-slash' : 'eye'\" />\n        </button>\n      </div>\n      <div v-if=\"errors.confirmPassword\" class=\"error-message\">\n        {{ errors.confirmPassword }}\n      </div>\n    </div>\n\n    <!-- Êèê‰∫§ÊåâÈíÆ -->\n    <button\n      type=\"submit\"\n      class=\"auth-btn\"\n      :disabled=\"isLoading || !isFormValid\"\n      :class=\"{ 'loading': isLoading }\"\n    >\n      <font-awesome-icon v-if=\"isLoading\" icon=\"spinner\" spin class=\"btn-icon\" />\n      <font-awesome-icon v-else icon=\"user-plus\" class=\"btn-icon\" />\n      {{ isLoading ? 'Ê≥®ÂÜå‰∏≠...' : 'Ê≥®ÂÜå' }}\n    </button>\n\n    <!-- ÈîôËØØ/ÊàêÂäüÊ∂àÊÅØ -->\n    <div v-if=\"message.text\" :class=\"['message', message.type]\">\n      <font-awesome-icon \n        :icon=\"message.type === 'error' ? 'exclamation-circle' : 'check-circle'\" \n        class=\"message-icon\" \n      />\n      {{ message.text }}\n    </div>\n\n    <!-- ÂàáÊç¢Âà∞ÁôªÂΩï -->\n    <div class=\"switch-mode\">\n      Â∑≤ÊúâË¥¶Âè∑Ôºü\n      <button type=\"button\" class=\"switch-link\" @click=\"$emit('switch-mode', 'login')\">\n        Á´ãÂç≥ÁôªÂΩï\n      </button>\n    </div>\n  </form>\n</template>\n\n<script>\nimport { ref, computed, reactive } from 'vue'\nimport { FontAwesomeIcon } from '@fortawesome/vue-fontawesome'\n\nexport default {\n  name: 'RegisterForm',\n  components: {\n    FontAwesomeIcon\n  },\n  emits: ['switch-mode', 'submit'],\n  setup(props, { emit }) {\n    // Ë°®ÂçïÊï∞ÊçÆ\n    const formData = reactive({\n      phoneNumber: '',\n      nickname: '',\n      password: '',\n      confirmPassword: ''\n    })\n\n    // Ë°®ÂçïÁä∂ÊÄÅ\n    const isLoading = ref(false)\n    const showPassword = ref(false)\n    const showConfirmPassword = ref(false)\n    const errors = reactive({\n      phoneNumber: '',\n      nickname: '',\n      password: '',\n      confirmPassword: ''\n    })\n    const message = reactive({\n      text: '',\n      type: ''\n    })\n\n    // È™åËØÅÊâãÊú∫Âè∑\n    const validatePhoneNumber = () => {\n      const phoneRegex = /^1[3-9]\\d{9}$/\n      if (!formData.phoneNumber) {\n        errors.phoneNumber = 'ËØ∑ËæìÂÖ•ÊâãÊú∫Âè∑'\n      } else if (!phoneRegex.test(formData.phoneNumber)) {\n        errors.phoneNumber = 'ËØ∑ËæìÂÖ•Ê≠£Á°ÆÁöÑ11‰ΩçÊâãÊú∫Âè∑'\n      } else {\n        errors.phoneNumber = ''\n      }\n    }\n\n    // È™åËØÅÊòµÁß∞\n    const validateNickname = () => {\n      if (formData.nickname && formData.nickname.length > 50) {\n        errors.nickname = 'ÊòµÁß∞ÈïøÂ∫¶‰∏çËÉΩË∂ÖËøá50‰∏™Â≠óÁ¨¶'\n      } else {\n        errors.nickname = ''\n      }\n    }\n\n    // È™åËØÅÂØÜÁ†Å\n    const validatePassword = () => {\n      if (!formData.password) {\n        errors.password = 'ËØ∑ËæìÂÖ•ÂØÜÁ†Å'\n      } else if (formData.password.length < 6) {\n        errors.password = 'ÂØÜÁ†ÅÈïøÂ∫¶Ëá≥Â∞ë6‰Ωç'\n      } else {\n        errors.password = ''\n      }\n      // Â¶ÇÊûúÁ°ÆËÆ§ÂØÜÁ†ÅÂ∑≤Â°´ÂÜôÔºåÈáçÊñ∞È™åËØÅÁ°ÆËÆ§ÂØÜÁ†Å\n      if (formData.confirmPassword) {\n        validateConfirmPassword()\n      }\n    }\n\n    // È™åËØÅÁ°ÆËÆ§ÂØÜÁ†Å\n    const validateConfirmPassword = () => {\n      if (!formData.confirmPassword) {\n        errors.confirmPassword = 'ËØ∑Á°ÆËÆ§ÂØÜÁ†Å'\n      } else if (formData.password !== formData.confirmPassword) {\n        errors.confirmPassword = '‰∏§Ê¨°ËæìÂÖ•ÁöÑÂØÜÁ†Å‰∏ç‰∏ÄËá¥'\n      } else {\n        errors.confirmPassword = ''\n      }\n    }\n\n    // Ë°®ÂçïÊòØÂê¶ÊúâÊïà\n    const isFormValid = computed(() => {\n      return formData.phoneNumber && \n             formData.password && \n             formData.confirmPassword &&\n             !errors.phoneNumber && \n             !errors.nickname &&\n             !errors.password && \n             !errors.confirmPassword\n    })\n\n    // ÊòæÁ§∫Ê∂àÊÅØ\n    const showMessage = (text, type = 'error') => {\n      console.log(`${type === 'error' ? '‚ùå' : '‚úÖ'} ${text}`)\n      message.text = text\n      message.type = type\n      setTimeout(() => {\n        message.text = ''\n        message.type = ''\n      }, 3000)\n    }\n\n    // Ë°®ÂçïÊèê‰∫§\n    const handleSubmit = async () => {\n      console.log('üîÑ Ê≥®ÂÜåË°®ÂçïÊèê‰∫§')\n      \n      // È™åËØÅË°®Âçï\n      validatePhoneNumber()\n      validateNickname()\n      validatePassword()\n      validateConfirmPassword()\n\n      if (!isFormValid.value) {\n        showMessage('ËØ∑Ê≠£Á°ÆÂ°´ÂÜôÊâÄÊúâÂ≠óÊÆµ')\n        return\n      }\n\n      isLoading.value = true\n\n      try {\n        const submitData = {\n          phoneNumber: formData.phoneNumber,\n          password: formData.password\n        }\n        \n        // Â¶ÇÊûúÂ°´ÂÜô‰∫ÜÊòµÁß∞ÔºåÊ∑ªÂä†Âà∞Êèê‰∫§Êï∞ÊçÆ‰∏≠\n        if (formData.nickname.trim()) {\n          submitData.nickname = formData.nickname.trim()\n        }\n\n        console.log('üì§ Êèê‰∫§Ê≥®ÂÜåÊï∞ÊçÆ:', { \n          ...submitData,\n          password: '***'\n        })\n        \n        await emit('submit', submitData)\n        showMessage('Ê≥®ÂÜåÊàêÂäüÔºÅ', 'success')\n        \n      } catch (error) {\n        console.error('‚ùå Ê≥®ÂÜåÂ§±Ë¥•:', error)\n        const errorMessage = error.response?.data?.message || error.message || 'Ê≥®ÂÜåÂ§±Ë¥•ÔºåËØ∑ÈáçËØï'\n        showMessage(errorMessage)\n      } finally {\n        isLoading.value = false\n      }\n    }\n\n    return {\n      formData,\n      errors,\n      message,\n      isLoading,\n      showPassword,\n      showConfirmPassword,\n      isFormValid,\n      validatePhoneNumber,\n      validateNickname,\n      validatePassword,\n      validateConfirmPassword,\n      handleSubmit,\n      showMessage\n    }\n  }\n}\n</script>\n\n<style scoped>\n.auth-form {\n  width: 100%;\n}\n\n.form-group {\n  margin-bottom: 20px;\n  text-align: left;\