ubtask.title || '';\n        this.formData.quadrant = this.subtask.quadrant || 2;\n        this.formData.date = this.subtask.date ? this.formatDateForInput(this.subtask.date) : new Date().toISOString().split('T')[0];\n        this.formData.time = this.subtask.time || '';\n      } else {\n        this.formData = {\n          title: '',\n          date: new Date().toISOString().split('T')[0],\n          time: '',\n          quadrant: 2\n        };\n      }\n    },\n    handleSubmit() {\n      if (!this.formData.title.trim()) {\n        return;\n      }\n      console.log('üöÄ [SubtaskModal] Êèê‰∫§Â≠ê‰ªªÂä°Êï∞ÊçÆ:', this.formData);\n      const submitData = {\n        title: this.formData.title.trim(),\n        quadrant: this.formData.quadrant,\n        date: this.formData.date,\n        time: this.formData.time,\n        collectionId: this.collectionId,\n        completed: false,\n        createdAt: new Date().toISOString(),\n        // Ê†πÊçÆÊó∂Èó¥Âà§Êñ≠timeBlockType\n        timeBlockType: this.getTimeBlockType(this.formData.time),\n        isScheduled: !!this.formData.time\n      };\n      if (this.isEditing) {\n        submitData._id = this.subtask._id;\n      }\n      console.log('üì§ [SubtaskModal] ÊúÄÁªàÊèê‰∫§Êï∞ÊçÆ:', submitData);\n      this.$emit('submit', submitData);\n    },\n    getTimeBlockType(time) {\n      if (!time) return 'unscheduled';\n      const hour = parseInt(time.split(':')[0]);\n      if (hour >= 7 && hour < 12) {\n        return 'morning';\n      } else if (hour >= 12 && hour < 18) {\n        return 'afternoon';\n      } else if (hour >= 18 && hour < 24) {\n        return 'evening';\n      }\n      return 'unscheduled';\n    },\n    handleClose() {\n      console.log('‚ùå [SubtaskModal] ÂÖ≥Èó≠ÂºπÁ™ó');\n      this.$emit('close');\n    },\n    handleKeyDown(event) {\n      if (event.key === 'Escape') {\n        this.handleClose();\n      }\n    },\n    formatDateForInput(date) {\n      if (!date) return '';\n      const d = new Date(date);