{
  "name": "ai-siri-intent-recognition",
  "version": "1.0.0",
  "description": "基于LangChain的AI Siri意图识别服务",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "ai",
    "siri",
    "intent-recognition",
    "langchain",
    "nlp",
    "chatbot",
    "qwen"
  ],
  "author": "AI Siri Team",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^0.3.51",
    "@langchain/core": "^0.3.71",
    "@langchain/openai": "^0.6.7",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "langchain": "^0.1.25",
    "mongoose": "^8.17.1",
    "openai": "^4.20.1",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/logs/**"
    ],
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/__tests__/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ai-siri-intent-recognition.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/ai-siri-intent-recognition/issues"
  },
  "homepage": "https://github.com/your-org/ai-siri-intent-recognition#readme"
}
